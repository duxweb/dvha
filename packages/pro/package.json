{
  "name": "@duxweb/dvha-pro",
  "type": "module",
  "version": "1.1.20",
  "author": "DuxWeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duxweb/dvha.git"
  },
  "exports": {
    ".": {
      "dvha:development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts"
      },
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./style.css": {
      "import": "./dist/esm/dvha-pro.css",
      "require": "./dist/cjs/dvha-pro.css"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && vue-tsc --noEmit && pnpm run build:uno && vite build && pnpm run build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:uno": "unocss -c uno.config.ts './src/**/*' -o ./src/theme/uno.css",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "@unocss/core": "^66.5.5"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.1",
    "@duxweb/dvha-core": "workspace:*",
    "@duxweb/dvha-naiveui": "workspace:*",
    "@iconify-json/tabler": "^1.2.23",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@overlastic/vue": "^0.8.3",
    "@tanstack/vue-query": "^5.90.7",
    "@unocss/preset-icons": "^66.5.5",
    "@unocss/preset-typography": "^66.5.5",
    "@unocss/runtime": "^66.5.5",
    "@vee-validate/i18n": "^4.15.1",
    "@vee-validate/rules": "^4.15.1",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.4",
    "@vue-flow/node-resizer": "^1.5.0",
    "@vue-flow/node-toolbar": "^1.1.1",
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "ace-builds": "^1.43.4",
    "aieditor": "^1.4.2",
    "clsx": "^2.1.1",
    "colorizr": "^3.0.8",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "fabric": "^6.7.1",
    "highlight.js": "^11.11.1",
    "jinrishici": "^1.0.6",
    "jsep": "^1.4.0",
    "lodash-es": "^4.17.21",
    "mathjs": "^15.1.0",
    "mime": "^4.1.0",
    "naive-ui": "^2.43.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "short-unique-id": "^5.3.2",
    "unocss": "^66.5.5",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-command-palette": "^0.2.3",
    "vue-cropper": "^1.1.4",
    "vue-draggable-plus": "^0.6.0",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.6.3",
    "vue3-ace-editor": "^2.2.4"
  },
  "devDependencies": {
    "@unocss/cli": "^66.5.5",
    "@unocss/core": "^66.5.5",
    "rimraf": "^6.1.0",
    "sass-embedded": "^1.93.3",
    "tslib": "^2.8.1",
    "vue-tsc": "^3.1.3"
  }
}
